<script type="text/javascript">
function selectVersion() {
	var myselect = document.getElementById("versionSelect");
	var version = myselect.options[myselect.selectedIndex].value;
	window.location.href = location.protocol + "//" + window.location.host + "/"+version;
}
function selectLang() {
	var myselect = document.getElementById("langSelect");
	var lang = myselect.options[myselect.selectedIndex].value;
	console.log(lang);
	window.location.href = location.protocol + "//" + window.location.host + "/{{ site.branch }}/lang#lang-"+lang;
}
</script>
